  "shortcuts": [
    {
      "name": "Add New Item",
      "short_name": "Add Item",
      "description": "Quickly add a new FF&E item",
      "url": "/add-item",
      "icons": [
        {
          "src": "/shortcut-add.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "View Projects",
      "short_name": "Projects", 
      "description": "View all projects",
      "url": "/projects",
      "icons": [
        {
          "src": "/shortcut-projects.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Scan Product",
      "short_name": "Scan",
      "description": "Scan product barcode or QR code",
      "url": "/scan",
      "icons": [
        {
          "src": "/shortcut-scan.png", 
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://establisheddesignco.com"
    }
  ],
  "prefer_related_applications": false,
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+ffe",
      "url": "/handle-protocol?url=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls", ".xlsx"]
      }
    }
  ],
  "handle_links": "preferred",
  "capture_links": "new-client"
}